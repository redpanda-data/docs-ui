            },
            header({items, html}) {
              if (!items.length) {
                return;
              }
              let headerTitle = "Add filters";
              return html`
              <span class="aa-SourceHeaderTitle">${headerTitle}</span>
              <div class="aa-SourceHeaderLine"></div>`;
            },
