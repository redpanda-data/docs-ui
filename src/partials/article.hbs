<article class="doc">
{{#if (ne page.version page.component.latest.version)}}
  {{> latest-banner}}
{{/if}}
{{#with page.title}}
<h1 class="page">{{{this}}}</h1>
{{/with}}
{{> page-versions}}
{{#if (eq page.layout 'home')}}
  {{> home}}
{{else if (eq page.layout 'index')}}
  {{> index}}
{{else}}
  {{> context-switcher}}
  {{{page.contents}}}
{{/if}}
{{> feedback-footer}}
{{> pagination}}
</article>
