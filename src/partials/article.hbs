<article class="doc">
{{#unless page.attributes.cloud}}
{{> latest-banner}}
  {{> page-versions}}
{{/unless}}
{{#with page.title}}
<h1 class="page">{{{this}}}</h1>
{{/with}}
{{#if page.attributes.cloud}}
  {{#if page.attributes.beta}}
    <img src="{{{uiRootPath}}}/img/cloud-beta.svg"> </img>
  {{/if}}
{{/if}}
{{#if (eq page.layout 'home')}}
  {{> home}}
{{else if (eq page.layout 'index')}}
  {{> index}}
{{else}}
  {{> context-switcher}}
  {{#with (add-suggested-labs page.attributes page.contents)  as |suggestedLabs|}}
    {{{suggestedLabs}}}
  {{/with}}
{{/if}}
{{> feedback-footer}}
{{> pagination}}
</article>
