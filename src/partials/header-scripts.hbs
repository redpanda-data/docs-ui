    "#post-/v1alpha2/topics": "/api/cloud-dataplane-api#post-/v1alpha2/topics",
    "#get-/v1alpha2/redpanda-connect/pipelines/-id-": "/api/cloud-dataplane-api#get-/v1alpha2/redpanda-connect/pipelines/-id-",
