{{#with page.versions}}
<div class="nav-panel-explore{{#unless @root.page.navigation}} is-active{{/unless}}" data-panel="explore" title="View other versions of {{{@root.page.component.name}}}">
  <div class="context">
    <div class="container has-dropdown">
      <span class="current-version">Version {{@root.page.componentVersion.displayVersion}}</span>
      <div class="versions">
        {{#if (ne @root.page.componentVersion.displayVersion 'default')}}
            {{#each this}}
              {{#if (ne ./version @root.page.version)}}
                <a class="version
                {{~#if ./missing}} is-missing{{/if}}" href="{{{relativize ./url}}}">{{./displayVersion}}</a>
              {{/if}}
            {{/each}}
        {{/if}}
      </div>
    </div>
  </div>
</div>
{{/with}}
