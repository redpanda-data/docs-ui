<section class="feedback-section">
  {{> thumbs}}
  <div class="col">
    <a href="https://redpanda.com/slack" alt="Slack Community">
      <span class="material-icons MuiIcon-root mailIcon_fWLs" aria-hidden="true">group</span> Ask in the community
    </a>
  </div>
  <div class="col">
    <a href="mailto:rp-docs-feedback@redpanda.com?subject=Documentation Feedback&amp;body=Doc url: https://docs.redpanda.com/docs/get-started/">
      <span class="material-icons MuiIcon-root mailIcon_fWLs" aria-hidden="true">email</span>
      <span> Share your feedback</span>
    </a>
  </div>
  <div class="col">
    {{#if (and page.fileUri (not env.CI))}}
    <a href="{{page.fileUri}}" target="_blank" rel="noreferrer noopener">
      <svg aria-hidden="true" class="octicon mr-1" viewBox="0 0 16 16" width="16" height="16" fill="currentColor" style="display: inline-block; user-select: none; vertical-align: text-bottom; overflow: visible;"><path fill-rule="evenodd" d="M7.177 3.073 9.573.677A.25.25 0 0 1 10 .854v4.792a.25.25 0 0 1-.427.177L7.177 3.427a.25.25 0 0 1 0-.354zM3.75 2.5a.75.75 0 1 0 0 1.5.75.75 0 0 0 0-1.5zm-2.25.75a2.25 2.25 0 1 1 3 2.122v5.256a2.251 2.251 0 1 1-1.5 0V5.372A2.25 2.25 0 0 1 1.5 3.25zM11 2.5h-1V4h1a1 1 0 0 1 1 1v5.628a2.251 2.251 0 1 0 1.5 0V5A2.5 2.5 0 0 0 11 2.5zm1 10.25a.75.75 0 1 1 1.5 0 .75.75 0 0 1-1.5 0zM3.75 12a.75.75 0 1 0 0 1.5.75.75 0 0 0 0-1.5z"></path>
      </svg>
      <span>Make a contribution</span>
    </a>
    {{else if (and page.editUrl (or env.FORCE_SHOW_EDIT_PAGE_LINK (not page.origin.private)))}}
    <a href="{{page.editUrl}}" target="_blank" rel="noreferrer noopener">
      <svg aria-hidden="true" class="octicon mr-1" viewBox="0 0 16 16" width="16" height="16" fill="currentColor" style="display: inline-block; user-select: none; vertical-align: text-bottom; overflow: visible;"><path fill-rule="evenodd" d="M7.177 3.073 9.573.677A.25.25 0 0 1 10 .854v4.792a.25.25 0 0 1-.427.177L7.177 3.427a.25.25 0 0 1 0-.354zM3.75 2.5a.75.75 0 1 0 0 1.5.75.75 0 0 0 0-1.5zm-2.25.75a2.25 2.25 0 1 1 3 2.122v5.256a2.251 2.251 0 1 1-1.5 0V5.372A2.25 2.25 0 0 1 1.5 3.25zM11 2.5h-1V4h1a1 1 0 0 1 1 1v5.628a2.251 2.251 0 1 0 1.5 0V5A2.5 2.5 0 0 0 11 2.5zm1 10.25a.75.75 0 1 1 1.5 0 .75.75 0 0 1-1.5 0zM3.75 12a.75.75 0 1 0 0 1.5.75.75 0 0 0 0-1.5z"></path>
      </svg>
      <span>Make a contribution</span>
    </a>
    {{/if}}
  </div>
</section>
